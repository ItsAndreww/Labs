#include <iostream>
#include <math.h>
using namespace std;

int main()
{
	double x, y, z, a, b, c, d, v;
	cout << "Enter x,y,z,a,b,c,d" << endl;
	cin >> x >> y >> z >> a >> b >> c >> d;
	if (a > b)
	{
		swap(a, b);
	}
	if (c > d)
	{
		swap(c, d);
	}
	if ((x >= c && x <= d) && (y >= c && y <= d) and z != 0)
	{
		v = (abs(x) + abs(y)) / z;
		cout << "Result is:" << v;
	}
	else if ((x >= c && x <= d) && (y >= c && y <= d) && (z == 0))
	{
		v = min(x, y) + sqrt(pow(x, 2) + pow(y, 2));
		cout << "Result is:" << v;
	}
	else
	{
		v = (max(abs(x), abs(y), abs(z)));
		cout << "Result is:" << v;
	}
	return 0;
}
